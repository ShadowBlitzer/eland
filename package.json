{
  "name": "eLAND",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eeemarv/eland.git"
  },
  "bugs": "https://github.com/eeemarv/eland/issues",
  "engines": {
    "node": "9",
    "npm": "5"
  },
  "devDependencies": {},
  "scripts": {
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:publish": "npm run docs:build && cd _book && git init && git commit --allow-empty -m 'Update docs' && git checkout -b gh-pages && git add --all && git commit -am 'Update docs' && git push https://github.com/eeemarv/eland gh-pages --force"
  },
  "dependencies": {
    "@symfony/webpack-encore": "^0.17.0",
    "autoprefixer": "^7.2.3",
    "bootstrap": "^4.0.0-beta.3",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "gitbook-cli": "^2.3.2",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "popper.js": "^1.12.9",
    "postcss-loader": "^2.0.9",
    "public-cdn-webpack-plugin": "^0.1.5",
    "sass-loader": "^6.0.6"
  }
}
