{% extends 'eland.html.twig' %}

{% import 'macro.html.twig' as mcr %}

{% block inner_content %}

<h1>{{ 'hosting.title'|trans }}</h1>

{{ form_start(form) }}

{{ form_row(form.group_name) }}

{{ form_row(form.email) }}

{{ form_row(form.message, {'attr': {'rows': 6}}) }}

{{ form_row(form.token) }}

{{ form_widget(form.send) }}

<br>
<p>{{ 'hosting.privacy'|trans }}</p>

{% endblock inner_content %}




